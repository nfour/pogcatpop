@import url("http://fonts.googleapis.com/css?family=Open+Sans:300,400")

body
	font-family	: "Open Sans", Arial
	font-size	: 16px
	font-weight	: 300
	color		: #444
	background	: #fff
	
table
	width: 50%
	margin: 50px auto
	
	th
		font-weight: 400
		text-align: left
		
	th, td
		padding: 8px 0
		line-height: 1.6em
		border-bottom: 1px dotted #aaa4